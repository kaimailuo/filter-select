{
  "name": "filter-select",
  "version": "0.1.0",
  "private": true,
  "main": "./lib/vcomponents.common.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
